{
    "name": "easy-angular",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "add-page": "ng g m pages/page-name --routing && ng g component pages/page-name --skip-tests",
        "build": "ng build --configuration development",
        "clean": "rm -rf dist .turbo .angular",
        "dev": "ng serve --port 5004",
        "gh-pages-build": "ng build --configuration development --base-href '/vechain-dapp-kit/angular/'",
        "preview": "ng serve --port 5004",
        "purge": "yarn clean; rm -rf node_modules",
        "watch": "ng build --watch --configuration development"
    },
    "dependencies": {
        "@angular/animations": "^18.2.13",
        "@angular/common": "^18.2.13",
        "@angular/compiler": "^18.2.13",
        "@angular/core": "^18.2.13",
        "@angular/forms": "^18.2.13",
        "@angular/localize": "^18.2.13",
        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "@caliatys/array-typer": "^1.0.0",
        "@ng-bootstrap/ng-bootstrap": "^15.1.2",
        "@ngx-translate/core": "^16.0.4",
        "@ngx-translate/http-loader": "^16.0.1",
        "@popperjs/core": "^2.11.8",
        "@vechain/dapp-kit": "workspace:^",
        "@vechain/dapp-kit-ui": "workspace:^",
        "angular-svg-icon": "^16.1.0",
        "assert": "2.1.0",
        "axios": "^1.8.4",
        "bootstrap": "^5.3.5",
        "buffer": "6.0.3",
        "crypto-browserify": "3.12.1",
        "https-browserify": "1.0.0",
        "os-browserify": "0.3.0",
        "rxjs": "~7.8.2",
        "stream-browserify": "3.0.0",
        "stream-http": "3.2.0",
        "tslib": "^2.8.1",
        "url": "0.11.4",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.17",
        "@angular/cli": "^18.2.17",
        "@angular/compiler-cli": "^18.2.13",
        "@types/jasmine": "~5.1.7",
        "@types/node": "^22.14.0",
        "jasmine-core": "~5.6.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "5.5.4"
    }
}

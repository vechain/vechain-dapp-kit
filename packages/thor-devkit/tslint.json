{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "no-namespace": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false
    },
    "rulesDirectory": []
}
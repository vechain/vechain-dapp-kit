{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React App",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES6"
  }
}
